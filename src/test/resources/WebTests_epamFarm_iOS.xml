<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Mobile tests" parallel="methods" thread-count="1">
<parameter name="propertyFile" value="/remote/iOS/webtest.properties"/>
<test name="Web Tests">
    <groups>
        <run>
            <include name="setup"/>
            <include name="web"/>
        </run>
    </groups>

    <classes>
        <class name="scenarios.webTests.SimpleWebTest"/>
    </classes>
</test>
</suite>